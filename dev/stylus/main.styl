@keyframes animate-bg{
	0%{
			transform scale(0) translateY(0)
			opacity 0
	}
		50%{
			opacity 1
	}
	100%{
			transform scale(1) translateY(-100px)
			opacity 0
	}
}
@keyframes animate-bg2{
	0%{
			transform scale(0) translateY(0) rotate(0deg)
			opacity 0
	}
		50%{
			opacity 1
	}
	100%{
			transform scale(1) translateY(-100px) rotate(360deg)
			opacity 0
	}
}
@keyframes animate-bg3{
	0%{
			opacity 0
	}
		50%{
			opacity .4
	}
	100%{
			opacity 0
	}
}
@keyframes animate-bg4{
	0%{
			opacity 0
	}
		50%{
			opacity 1
	}
	100%{
			opacity 0
	}
}
